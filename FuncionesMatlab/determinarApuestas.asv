function c=determinarApuestas(liga, nApuesta,archivoPartidos,archivoApuestas)
% %Liga
% 1 Espana
% 2 Inglaterra
% 3 Alemania
% 4 Italia
% 5 Francia

c=cell(10,5);

[ptas,pathAG]=encontrarPaths;
s=[ptas{liga},archivoPartidos];
[n,t,x]=xlsread(s);

eq=x(:,1:2);

ap=load([pathAG,archivoApuestas]);
ap=ap/100;

nap=nApuesta*ap;
size(nap)
c(:,1:2)=eq;
c(:,3:5)=mat2cell(nap,10,3);
end