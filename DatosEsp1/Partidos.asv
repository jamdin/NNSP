function [D,dat] = Partidos(datos,valorparametro,numpartido)
%datos del equipo
%valorparametro: numero de partidos
%numpartido: a partir de cual partido
r=find(strcmp(datos(:,1),numpartido));%encuentra el numero de fila del partido

if r<valorparametro
   valorparametro=r; 
end
dat=datos((r-valorparametro+1):r,:);
D=resultados(dat);
end